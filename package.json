{
  "name": "archive",
  "type": "module",
  "private": true,
  "imports": {
    "#/*": "./src/*",
    "#tools/*": "./tools/*"
  },
  "scripts": {
    "db:migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:schema": "prisma migrate dev",
    "dev": "bun run --watch src/index.ts",
    "import": "bun run tools/importLocal.ts",
    "importRemote": "bun run tools/importRemote.ts",
    "start": "bun run src/index.ts"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/static": "^8.3.0",
    "@fastify/view": "^11.1.1",
    "connect-redis": "^9.0.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "fastify": "^5.6.2",
    "fflate": "^0.8.2",
    "kysely": "^0.28.8",
    "mysql2": "^3.15.3",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "bun-types": "^1.3.4",
    "prisma": "^6.19.0",
    "prisma-kysely": "^2.2.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
