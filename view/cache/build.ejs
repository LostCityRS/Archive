<!doctype html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Lost City Archive</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/png" href="/favicon.png">

    <script src="https://cdn.jsdelivr.net/npm/@tailwindcss/browser@4"></script>
</head>

<body class="bg-gray-800 text-white">
    <%= cache.build %><br>
    <%= files.length %> files<br>

    <% if (cache.js5) { %>
        missing <%= missing.length %> files:<br>

        <% for (const file of missing) { %>
            Archive: <%= file.archive %><br>
            Group: <%= file.group %><br>
            Version: <%= file.version %><br>
            CRC: <%= file.crc %><br>
            <br>
        <% } %>
    <% } else if (cache.ondemand) { %>
        missing <%= missing.length %> essential files:<br>

        <% for (const file of missing) { %>
            Archive: <%= file.archive %><br>
            File: <%= file.file %><br>
            Version: <%= file.version %><br>
            CRC: <%= file.crc %><br>
            <br>
        <% } %>
    <% } else if (cache.jag) { %>
        missing <%= missing.length %> files:<br>

        <% for (const file of missing) { %>
            Name: <%= file.name %><br>
            CRC: <%= file.crc %><br>
            <br>
        <% } %>
    <% } %>
</body>

</html>
