<%
	const isActive =
		!hideActiveClasses &&
    	title &&
    	match &&
    	title.toLowerCase().includes(match.toLowerCase());

	const activeClasses = "bg-white/20 md:bg-black/20 text-harvest-gold-400";
%>

<li>
	<a href="<%= href %>"
		 class="<%= isActive ? activeClasses : '' %>">
		<%= label %>
	</a>
</li>